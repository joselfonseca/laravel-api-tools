{
    "name": "joselfonseca/laravel-api-tools",
    "description": "Some tools for Laravel API development, nothing really new, just wrappers and helper stuff",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Jose Luis Fonseca",
            "email": "jose@ditecnologia.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-fractal": "^6.0",
        "api-ecosystem-for-laravel/dingo-api": "^4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1",
        "orchestra/testbench": "^8.0",
        "mockery/mockery": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "Joselfonseca\\LaravelApiTools\\": "src/"
        }
    },
    "autoload-dev": {
      "psr-4": {
        "Joselfonseca\\LaravelApiTools\\Tests\\": "tests/"
      }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Joselfonseca\\LaravelApiTools\\Providers\\LaravelApiToolsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist"
    }
}
